<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.orangead.mock-webcam</string>

    <key>Program</key>
    <string>/Users/admin/orangead/orangead-mock-webcam/start-dual-osascript.sh</string>

    <key>WorkingDirectory</key>
    <string>/Users/admin/orangead/orangead-mock-webcam</string>

    <key>StandardOutPath</key>
    <string>/Users/admin/logs/mock-webcam.log</string>

    <key>StandardErrorPath</key>
    <string>/Users/admin/logs/mock-webcam.error.log</string>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <false/>

    <key>EnvironmentVariables</key>
    <dict>
        <key>PATH</key>
        <string>/opt/homebrew/bin:/usr/local/bin:/usr/bin:/bin</string>
        <key>HOME</key>
        <string>/Users/admin</string>
        <key>USER</key>
        <string>admin</string>
    </dict>

    <key>LimitLoadToSessionType</key>
    <array>
        <string>Aqua</string>
    </array>

    <key>ProcessType</key>
    <string>Background</string>

    <key>ExitTimeOut</key>
    <integer>30</integer>

    <key>StandardOutPath</key>
    <string>/Users/admin/orangead/logs/mock-webcam.log</string>

    <key>StandardErrorPath</key>
    <string>/Users/admin/orangead/logs/mock-webcam.error.log</string>

</dict>
</plist>